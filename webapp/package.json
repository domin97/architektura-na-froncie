    "msw": "^1.2.2",
    "msw-storybook-addon": "^1.8.0",
    "node-polyfill-webpack-plugin": "^2.0.1",
    "polished": "^4.2.2",
    "prop-types": "^15.8.1",
    "puppeteer": "~18.1.0",
    "react": "^18.2.0",
    "react-app-rewired": "^2.2.1",
    "react-dom": "^18.2.0",
    "react-markdown": "^8.0.3",
    "react-modal": "^3.15.1",
    "react-redux": "^8.0.2",
    "react-router": "6.3.0",
    "react-router-dom": "6.3.0",
    "react-scripts": "^5.0.1",
    "redux": "^4.2.0",
    "redux-devtools-extension": "^2.13.9",
    "redux-mock-store": "^1.5.4",
    "redux-thunk": "^2.4.1",
    "request": "^2.88.2",
    "retry-axios": "^3.0.0",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.5",
    "rxjs-spy": "^8.0.2",
    "simplemde": "^1.11.2",
    "socket.io-client": "^4.4.1",
    "storybook": "^7.0.20",
    "styled-components": "^5.3.11",
    "ts-jest": "^27.1.5",
    "typescript": "~4.6.4",
    "uuid": "^8.3.2",
    "web-vitals": "^2.1.4",
    "webpack": "^5.73.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "xstate": "^4.38.0"
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "m2/setup",
      "m3/homework/contacts",
      "cypress",
      "puppeteer",
      "storyshots"
    ],
    "moduleNameMapper": {
      "^axios$": "axios/dist/axios.js"
    }
  },
  "madge": {
    "tsConfig": "tsconfig.json"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "engines": {
    "node": ">=16.14.0",
    "npm": ">=8.7.0"
  },
  "overrides": {
    "cypress": "^12.17.2",
    "jest": "^27.5.1",
    "puppeteer": "~18.1.0",
    "webpack": "^5.73.0"
  }
}
